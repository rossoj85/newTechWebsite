{
  "name": "NewTechHorizons",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://rossoj85.github.io/newTechWebsite",
  "dependencies": {
    "bootstrap": "^3.4.1",
    "gh-pages": "^6.0.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.14.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.14.0",
    "react-parallax": "^1.8.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.1",
    "react-scrollable-anchor": "^0.6.1"
  },
  "scripts": {
    "build-css": "sass src/index.scss src/index.css",
    "watch-css": "npm run build-css && sass src/index.scss:src/index.css --watch --no-source-map",
    "start-js": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "sass": "^1.69.5"
  }
}
